<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  
  {% for post in site.posts %}
  {% unless post.draft %}
  <url>
  	<loc>http://julienvey.com{{ post.url }}</loc>
  </url>
  {% endunless %}
  {% endfor %}

  {% for page in site.pages %}
  <url>
  	<loc>http://julienvey.com{{ page.url }}</loc>
  </url>
  {% endfor %}

  <url>
  	<loc>http://julienvey.com</loc>
  </url>
  
</urlset>