---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Jawher's Blog</title>
 <link href="http://jawher.org/atom.xml" rel="self"/>
 <link href="http://jawher.org/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://jawher.org/</id>
 <author>
   <name>Jawher Moussa</name>
   <email>firstName DOT lastName AT gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://jawher.org{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://jawher.org{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
